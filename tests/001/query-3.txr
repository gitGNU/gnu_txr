@/.*/@# skip header
@(collect)
@  (coll)@{TOKEN /[^ ]+/}@(end)
@  (bind (UID PID PPID C STIME TTY TIME . CMD) TOKEN)
@  (cat CMD) @#
@  (forget TOKEN)
@(end)
@(output)
@(repeat)
@{UID 8} @{PID -5}
@(first)
UID        PID
@{UID 8} @{PID -5}
@(empty)
No processes!
@(end)
@(end)
